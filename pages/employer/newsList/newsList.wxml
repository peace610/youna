<view class='container'>
    <view class='list' wx:for='{{list}}'>
        <view class='time'>{{item.create_time}}</view>
        <view class='area'>
            <view class='orderInfo'>
                <text class='read'>{{item.title}}</text>
                <!--<text class='tip'></text>-->
            </view>
            <navigator url="/pages/employer/orderDetail/orderDetail?id={{item.id}}"  class='des'>
                <text>{{item.context}}</text>
            </navigator>
        </view>
    </view>
</view>